
javapath jre1.3.1_10_win32/bin/java

# Initial heap size
vmparam -Xms32m

# Maximum Heap size
vmparam -Xmx512m

# Enable incremental garbage collection
vmparam -Xincgc

vmparam -XX:+ForceTimeHighResolution


# python path
vmparam -Dpython.home=jython-2.1
vmparam -Dpython.path=jython-2.1/Lib/alice

# jars
addjars lib
addjars externalLib
addjars optionalPackagesLib
addjars jre1.3.1_10_win32/lib

# jni libs
vmparam -Djava.library.path=lib/win32;externalLib/win32;optionalPackagesLib/win32

mainclass edu.cmu.cs.stage3.alice.authoringtool.JAlice -o -e
